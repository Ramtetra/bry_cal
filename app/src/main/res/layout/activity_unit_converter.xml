<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/back_color"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".UnitConverterActivity">
    <include
        android:id="@+id/toolbar"
        layout="@layout/app_bar"/>
    <LinearLayout
        android:id="@+id/layoutHeader"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:weightSum="6"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/layout1"
            android:orientation="vertical"
            android:layout_weight="1"
            android:background="@drawable/shape_border_radius"
            android:layout_width="0dp"
            android:layout_height="75dp">

            <ImageView
                android:src="@drawable/psycal_icon"
                android:paddingTop="5dp"
                android:layout_gravity="center"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:text="@string/psycal"
                android:textColor="@color/heading_color"
                android:textSize="8sp"
                android:fontFamily="@font/poppins_regular"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/layout2"
            android:orientation="vertical"
            android:layout_marginLeft="3dp"
            android:layout_weight="1"
            android:background="@drawable/shape_border_radius"
            android:layout_width="0dp"
            android:layout_height="75dp">

            <ImageView
                android:paddingTop="5dp"
                android:src="@drawable/mixed_air_icon"
                android:layout_gravity="center"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:text="@string/mixed_air_conditions"
                android:textColor="@color/heading_color"
                android:textSize="8sp"
                android:layout_gravity="center"
                android:fontFamily="@font/poppins_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/layout3"
            android:orientation="vertical"
            android:layout_marginLeft="3dp"
            android:layout_weight="1"
            android:background="@drawable/shape_border_radius_active"
            android:layout_width="0dp"
            android:layout_height="75dp">

            <ImageView
                android:layout_gravity="center"
                android:paddingTop="5dp"
                android:src="@drawable/unit_converter_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:text="@string/unit_converter"
                android:textColor="@color/heading_color"
                android:textSize="8sp"
                android:layout_gravity="center"
                android:fontFamily="@font/poppins_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="@string/converter"
                android:textColor="@color/heading_color"
                android:textSize="8sp"
                android:layout_gravity="center"
                android:fontFamily="@font/poppins_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout4"
            android:orientation="vertical"
            android:layout_marginLeft="3dp"
            android:layout_weight="1"
            android:background="@drawable/shape_border_radius"
            android:layout_width="0dp"
            android:layout_height="75dp">

            <ImageView
                android:layout_gravity="center"
                android:paddingTop="5dp"
                android:src="@drawable/ductulator_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:text="@string/ductulator"
                android:textColor="@color/heading_color"
                android:textSize="8sp"
                android:layout_gravity="center"
                android:fontFamily="@font/poppins_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout5"
            android:orientation="vertical"
            android:layout_marginLeft="3dp"
            android:layout_weight="1"
            android:background="@drawable/shape_border_radius"
            android:layout_width="0dp"
            android:layout_height="75dp">

            <ImageView
                android:layout_gravity="center"
                android:paddingTop="5dp"
                android:src="@drawable/co2_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:text="@string/co2"
                android:textColor="@color/heading_color"
                android:textSize="8sp"
                android:fontFamily="@font/poppins_regular"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout6"
            android:orientation="vertical"
            android:layout_marginLeft="3dp"
            android:layout_weight="1"
            android:background="@drawable/shape_border_radius"
            android:layout_width="0dp"
            android:layout_height="75dp">

            <ImageView
                android:layout_gravity="center"
                android:paddingTop="5dp"
                android:src="@drawable/weather_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:text="@string/weather_bin_data"
                android:textColor="@color/heading_color"
                android:textSize="8sp"
                android:fontFamily="@font/poppins_regular"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

    </LinearLayout>
    <ScrollView
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:paddingBottom="30dp"
        android:layout_marginTop="190dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView"
            android:layout_marginTop="170dp"
            app:cardCornerRadius="8dp"
            android:layout_margin="10dp"
            android:paddingBottom="20dp"
            android:elevation="10dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="7dp"
                    android:weightSum="4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/cardLength"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_weight="1"
                        app:cardCornerRadius="8dp"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:src="@drawable/length_icon"
                                android:paddingTop="5dp"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Length"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:id="@+id/cardArea"
                        android:layout_marginLeft="7dp"
                        app:cardCornerRadius="8dp"
                        android:layout_weight="1"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:paddingTop="5dp"
                                android:src="@drawable/area_icon"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Area"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_marginLeft="7dp"
                        app:cardCornerRadius="8dp"
                        android:layout_weight="1"
                        android:id="@+id/cardVolume"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:paddingTop="5dp"
                                android:src="@drawable/volume_icon"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Volume"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_marginLeft="7dp"
                        app:cardCornerRadius="8dp"
                        android:layout_weight="1"
                        android:id="@+id/cardTemperature"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:paddingTop="5dp"
                                android:src="@drawable/temprature_icon"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Temperature"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="7dp"
                    android:weightSum="4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/cardPressure"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_weight="1"
                        app:cardCornerRadius="8dp"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:paddingTop="5dp"
                                android:src="@drawable/pressure_icon"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Pressure"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:layout_marginLeft="7dp"
                        app:cardCornerRadius="8dp"
                        android:layout_weight="1"
                        android:id="@+id/cardAirFlow"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:src="@drawable/air_icon"
                                android:layout_gravity="center"
                                android:paddingTop="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Air Flow"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_marginLeft="7dp"
                        app:cardCornerRadius="8dp"
                        android:layout_weight="1"
                        android:id="@+id/cardWeight"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:paddingTop="5dp"
                                android:src="@drawable/weight_icon"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Weight"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_marginLeft="7dp"
                        app:cardCornerRadius="8dp"
                        android:layout_weight="1"
                        android:id="@+id/cardPower"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:paddingTop="5dp"
                                android:src="@drawable/power_icon"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Power"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="7dp"
                    android:weightSum="4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.cardview.widget.CardView
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        app:cardCornerRadius="8dp"
                        android:id="@+id/cardEnergy"
                        android:layout_weight="0.32"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:paddingTop="5dp"
                                android:src="@drawable/energy_icon"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Energy"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:layout_marginLeft="7dp"
                        app:cardCornerRadius="8dp"
                        android:id="@+id/cardHumidity"
                        android:layout_weight="0.32"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:cardBackgroundColor="@color/light_yellow_card"
                        android:layout_width="70dp"
                        android:layout_height="70dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:paddingTop="5dp"
                                android:src="@drawable/humidiy_icon"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="Humidity"
                                android:textSize="10sp"
                                android:fontFamily="@font/poppins_medium"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_weight="2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="20dp"
                            android:background="@drawable/shape_rectangle"
                            android:layout_width="100dp"
                            android:layout_height="30dp">

                            <TextView
                                android:text="INPUT"
                                android:gravity="center"
                                android:layout_weight="1"
                                android:textSize="14sp"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:fontFamily="@font/poppins_bold"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:background="@drawable/shape_rectangel_green"
                            android:layout_width="100dp"
                            android:layout_height="30dp">

                            <TextView
                                android:text="OUTPUT"
                                android:gravity="center"
                                android:layout_weight="1"
                                android:textSize="14sp"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:fontFamily="@font/poppins_bold"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:weightSum="2"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            app:boxStrokeColor="@color/dark_blue"
                            android:hint="From"
                            app:layout_constraintTop_toBottomOf="@+id/layoutProjectName">

                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/etFrom"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="numberDecimal"
                                android:textSize="15sp"></com.google.android.material.textfield.TextInputEditText>
                        </com.google.android.material.textfield.TextInputLayout>


                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_weight="1"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:hint="To"
                            app:boxStrokeColor="@color/dark_blue"
                            app:layout_constraintTop_toBottomOf="@+id/layoutProjectName">

                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/etTo"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="numberDecimal"
                                android:textSize="15sp">
                            </com.google.android.material.textfield.TextInputEditText>
                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:weightSum="2"
                        android:layout_marginBottom="50dp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.google.android.material.textfield.TextInputLayout
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            app:boxStrokeColor="@color/dark_blue"
                            android:hint="@string/unit_converter"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/layoutMotorType">

                            <AutoCompleteTextView
                                android:id="@+id/etUnitInput"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins_regular"
                                android:inputType="none"
                                android:textSize="14sp" />

                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="10dp"
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            android:hint="@string/unit_converter"
                            app:boxStrokeColor="@color/dark_blue"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/layoutMotorType">

                            <AutoCompleteTextView
                                android:id="@+id/etUnitOutput"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins_regular"
                                android:inputType="none"
                                android:textSize="14sp" />

                        </com.google.android.material.textfield.TextInputLayout>

                    </LinearLayout>


                </LinearLayout>




            </LinearLayout>


        </androidx.cardview.widget.CardView>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>